{
  "name": "@rspress/mdx-rs",
  "description": "MDX compilation binding for Rspress",
  "version": "0.6.5",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "napi": {
    "name": "mdx-binding-core",
    "triples": {
      "defaults": false,
      "additional": [
        "aarch64-unknown-linux-musl",
        "x86_64-unknown-linux-musl",
        "aarch64-unknown-linux-gnu",
        "x86_64-unknown-linux-gnu",
        "x86_64-pc-windows-msvc",
        "aarch64-pc-windows-msvc",
        "aarch64-apple-darwin",
        "x86_64-apple-darwin"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "path-serializer": "0.2.2",
    "@mdx-js/mdx": "^2.3.0",
    "@napi-rs/cli": "^2.14.8",
    "benchmark": "^2.1.4",
    "cli-table": "^0.3.11",
    "conventional-changelog": "^3.1.25",
    "conventional-changelog-cli": "^2.2.2",
    "prettier": "^2.8.7",
    "typescript": "^5.0.2",
    "vite": "^4.4.11",
    "vitest": "^0.29.7",
    "@types/node": "16.18.101"
  },
  "engines": {
    "node": ">= 10"
  },
  "packageManager": "pnpm@8.7.0",
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --cargo-cwd crates/binding --platform --release",
    "build:debug": "napi build --cargo-cwd crates/binding --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "vitest run",
    "test:local": "vitest run",
    "test:watch": "vitest dev",
    "universal": "napi universal",
    "version": "napi version",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release-commit": "pnpm changelog && git add -A && git commit -m \"chore(release): publish v$npm_package_version\""
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  }
}
